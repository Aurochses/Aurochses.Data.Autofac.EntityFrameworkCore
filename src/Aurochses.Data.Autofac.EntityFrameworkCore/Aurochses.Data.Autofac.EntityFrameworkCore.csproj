<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <Authors>Aurochses</Authors>
    <Company>Aurochses</Company>
    <Copyright>Aurochses 2017</Copyright>
    <Description>Library for using Autofac in modules with Entity Framework Core.</Description>
    <PackageLicenseUrl>https://github.com/Aurochses/Aurochses.Data.Autofac.EntityFrameworkCore/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Aurochses/Aurochses.Data.Autofac.EntityFrameworkCore/wiki</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Aurochses/Aurochses.Data.Autofac.EntityFrameworkCore</RepositoryUrl>
    <PackageTags>Data Business Autofac Module EntityFrameworkCore</PackageTags>
    <PackageIconUrl>https://www.gravatar.com/avatar/cb5925a77e502401f95bf2738b123994?s=64</PackageIconUrl>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="4.8.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.1.4" />
  </ItemGroup>

</Project>
